{
  "name": "veel",
  "version": "1.0.0-beta.5",
  "description": "Base react styling components using fela with a design system",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "test": "ava"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "last 2 versions"
          }
        }
      ]
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "fela": "^5.0.2",
    "prop-types": "^15.5.10",
    "styled-system": "^1.0.0-7"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "react": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "author": "queckezz <fabian.eichenberger@gmail.com>",
  "license": "MIT",
  "repository": "queckezz/veel",
  "keywords": [
    "react-component",
    "design system",
    "fela",
    "styling",
    "css-in-js",
    "ui",
    "react"
  ]
}